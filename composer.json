{
  "name": "spiral/log-viewer",
  "description": "Vault component with ability to view logs in the vault panel.",
  "authors": [
    {
      "name": "Valentin V / vvval",
      "email": "vintsukevich@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "spiral/vault": "^0.9.7",
    "spiral/framework": "^1.0",
    "nesbot/carbon": "1.38",
    "vvval/spiral-array-paginable": "^0.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "mockery/mockery": "dev-master",
    "spiral/ide-helper": "^0.9.2",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Spiral\\": "source/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Spiral\\Tests\\": "tests/",
      "TestApplication\\": "tests/-app-/classes/"
    }
